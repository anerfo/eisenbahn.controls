            _Betriebsart = value

            Dim text As String = "Betriebsart "
            If _Betriebsart = 0 Then
                text = "Automatik ausgeschaltet"
            ElseIf _Betriebsart = 1 Then
                text = "Züge fahren auf Innenkreis und Außenkreis unabhängig"
