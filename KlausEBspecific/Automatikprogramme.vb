                If ComboBox1.SelectedIndex = 0 Then       ' *** Aus
                ElseIf ComboBox1.SelectedIndex = 1 Then   ' *** Eisenbahn

                    FilmZeile = FilmZeile - 1
                    If FilmZeile < FilmZeileMin Then
                        FilmZeile = FilmZeileMax
                    End If
                    FilmSpalte = 18

